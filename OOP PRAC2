public class StudentD {
    int StudentID;
    String Student_name;
    int marks;

    StudentD(int StudentID, String Student_name, int marks){
        this.marks=marks;
        this.Student_name=Student_name;
        this.StudentID= StudentID;
    }

    public void DisplayDetails(int StudentID, String Student_name, int marks){
        System.out.println("student id is:");
        System.out.println(this.StudentID);
        System.out.println("student name is: ");
        System.out.println(this.Student_name);
        System.out.println("student marks: ");
        System.out.println(this.marks);
    }

    public void grades(int marks){
        if (marks>40 && marks<50) {
            System.out.println("grade awarded is AA");
        }
        if (marks>30 && marks<40) {
            System.out.println("grade awarded is AB");
        }
        if (marks>20 && marks<30) {
            System.out.println("grade awarded is BB");
        }
        if (marks>10 && marks<20) {
            System.out.println("grade awarded is BC");
        }
        if (marks>0 && marks<10) {
            System.out.println("FAIL");
        }
    }


    public static void main(String[] args) {

        StudentD garima= new StudentD(47, "garima", 35);
        System.out.println("print details");
        garima.DisplayDetails(47, "garima", 35);
        garima.grades(35);
    }
}
