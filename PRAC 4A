public class CourseD {
    String subject_name;
    int marks;
    int credits;

    CourseD(String subject_name, int marks, int credits){

        this.subject_name=subject_name;
        this.credits=credits;
        this.marks=marks;
    }

    public static void main(String[] args) {
        CourseD [] details= new CourseD[4];

        details[0]= new CourseD("maths", 45,3);
        details[1]= new CourseD("phy", 35,2);
        details[2]= new CourseD("oops", 35,2);
        details[3]= new CourseD("algo", 41,3);

        for (CourseD student: details){
            System.out.println(" Subject: "+student.subject_name);
            System.out.println("marks:" +student.marks);
            System.out.println("credits:" +student.credits);
        }
    }
}
